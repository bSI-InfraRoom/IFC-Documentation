<?xml version="1.0"?>
<PropertySetDef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" ifdguid="af381200d1ec11e1800000215ad4efdf" templatetype="PSET_TYPEDRIVENOVERRIDE" xsi:noNamespaceSchemaLocation="http://buildingSMART-tech.org/xml/psd/PSD_IFC4.xsd">
  <IfcVersion version="IFC4x3_RC4" />
  <Name>Pset_MaterialCombustion</Name>
  <Definition>A set of extended material properties of products of combustion generated by elements typically used within the context of building services and flow distribution systems.</Definition>
  <Applicability />
  <ApplicableClasses>
    <ClassName>IfcMaterial</ClassName>
  </ApplicableClasses>
  <ApplicableTypeValue>IfcMaterial</ApplicableTypeValue>
  <PropertyDefs>
    <PropertyDef ifdguid="b6f7b680d1ec11e1800000215ad4efdf">
      <Name>SpecificHeatCapacity</Name>
      <Definition>Specific heat of the products of combustion: heat energy absorbed per temperature unit.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcSpecificHeatCapacityMeasure" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="en">Specific Heat Capacity</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="en" />
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="bc550100d1ec11e1800000215ad4efdf">
      <Name>N20Content</Name>
      <Definition>Nitrous oxide (N2O) content of the products of combustion. This is measured in weight of N2O per unit weight and is therefore unitless.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcPositiveRatioMeasure" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="en">N20 Content</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="en" />
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="c1b24b80d1ec11e1800000215ad4efdf">
      <Name>COContent</Name>
      <Definition>Carbon monoxide (CO) content of the products of combustion. This is measured in weight of CO per unit weight and is therefore unitless.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcPositiveRatioMeasure" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="en">COContent</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="en" />
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="c7a82c80d1ec11e1800000215ad4efdf">
      <Name>CO2Content</Name>
      <Definition>Carbon dioxide (CO2) content of the products of combustion. This is measured in weight of CO2 per unit weight and is therefore unitless.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcPositiveRatioMeasure" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="en">CO2 Content</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="en" />
      </DefinitionAliases>
    </PropertyDef>
  </PropertyDefs>
  <PsetDefinitionAliases>
    <PsetDefinitionAlias lang="en" />
  </PsetDefinitionAliases>
</PropertySetDef>