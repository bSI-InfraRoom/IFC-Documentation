            <ConstantDef>
              <Name>TWOWAY</Name>
              <NameAliases>
                <NameAlias lang="en">Two-way</NameAlias>
              </NameAliases>
              <DefinitionAliases />
            </ConstantDef>
            <ConstantDef>
              <Name>FOURWAY</Name>
              <NameAliases>
                <NameAlias lang="en">Four-way</NameAlias>
              </NameAliases>
              <DefinitionAliases />
            </ConstantDef>
            <ConstantDef>
              <Name>OTHER</Name>
              <Definition>required category not on scale</Definition>
              <NameAliases>
                <NameAlias lang="en">(other)</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en">Value is not listed.</DefinitionAlias>
              </DefinitionAliases>
