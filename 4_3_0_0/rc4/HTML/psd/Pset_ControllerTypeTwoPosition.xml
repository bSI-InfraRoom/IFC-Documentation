      </PropertyType>
      <NameAliases>
        <NameAlias lang="en">Polarity</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="en" />
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="2d38f080d1ce11e1800000215ad4efdf">
