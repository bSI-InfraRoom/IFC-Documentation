  <Name>Pset_SensorTypeMoistureSensor</Name>
  <Definition>A device that senses or detects moisture. HISTORY: Added in IFC4.</Definition>
  <Applicability />
  <ApplicableClasses>
    <ClassName>IfcSensor/MOISTURESENSOR</ClassName>
  </ApplicableClasses>
  <ApplicableTypeValue>IfcSensor/MOISTURESENSOR</ApplicableTypeValue>
  <PropertyDefs>
