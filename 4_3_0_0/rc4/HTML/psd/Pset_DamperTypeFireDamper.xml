    <PropertyDef ifdguid="bc93d300d1d111e1800000215ad4efdf">
      <Name>ActuationType</Name>
      <Definition>Enumeration that identifies the different types of dampers.</Definition>
      <PropertyType>
        <TypePropertyEnumeratedValue>
          <EnumList name="PEnum_FireDamperActuationType">
            <EnumItem>GRAVITY</EnumItem>
            <EnumItem>SPRING</EnumItem>
            <EnumItem>OTHER</EnumItem>
            <EnumItem>NOTKNOWN</EnumItem>
            <EnumItem>UNSET</EnumItem>
          </EnumList>
          <ConstantList>
            <ConstantDef>
              <Name>GRAVITY</Name>
              <NameAliases>
                <NameAlias lang="en">Gravity</NameAlias>
              </NameAliases>
              <DefinitionAliases />
            </ConstantDef>
            <ConstantDef>
              <Name>SPRING</Name>
              <NameAliases>
                <NameAlias lang="en">Spring</NameAlias>
              </NameAliases>
              <DefinitionAliases />
            </ConstantDef>
            <ConstantDef>
              <Name>OTHER</Name>
              <Definition>required category not on scale</Definition>
              <NameAliases>
                <NameAlias lang="en">(other)</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en">Value is not listed.</DefinitionAlias>
              </DefinitionAliases>
            </ConstantDef>
            <ConstantDef>
              <Name>NOTKNOWN</Name>
              <Definition>Value is unkown</Definition>
              <NameAliases>
                <NameAlias lang="en">(unknown)</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en">Value is unknown.</DefinitionAlias>
              </DefinitionAliases>
            </ConstantDef>
            <ConstantDef>
              <Name>UNSET</Name>
              <Definition>Value has not been specified</Definition>
              <NameAliases>
                <NameAlias lang="en">(unset)</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en">Value has not been specified.</DefinitionAlias>
              </DefinitionAliases>
            </ConstantDef>
          </ConstantList>
        </TypePropertyEnumeratedValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="en">Actuation Type</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="en" />
      </DefinitionAliases>
    </PropertyDef>
