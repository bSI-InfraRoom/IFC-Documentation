        <TypePropertyEnumeratedValue>
          <EnumList name="PEnum_CoolingTowerCircuitType">
            <EnumItem>OPENCIRCUIT</EnumItem>
            <EnumItem>CLOSEDCIRCUITWET</EnumItem>
