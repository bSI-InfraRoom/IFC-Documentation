            <ConstantDef>
              <Name>DOUBLECHECKBACKFLOWPREVENTER</Name>
              <NameAliases>
                <NameAlias lang="en">Double Check Backflow Preventer</NameAlias>
              </NameAliases>
              <DefinitionAliases />
            </ConstantDef>
            <ConstantDef>
              <Name>PRESSUREVACUUMBREAKER</Name>
              <NameAliases>
                <NameAlias lang="en">Pressure Vacuum Breaker</NameAlias>
              </NameAliases>
              <DefinitionAliases />
            </ConstantDef>
            <ConstantDef>
