<?xml version="1.0"?>
<PropertySetDef xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" ifdguid="8281fb80d1e811e1800000215ad4efdf" templatetype="PSET_TYPEDRIVENOVERRIDE" xsi:noNamespaceSchemaLocation="http://buildingSMART-tech.org/xml/psd/PSD_IFC4.xsd">
  <IfcVersion version="IFC4x3_RC4" />
  <Name>Pset_FlowMeterTypeWaterMeter</Name>
  <Definition>Device that measures, indicates and sometimes records, the volume of water that passes through it without interrupting the flow.</Definition>
  <Applicability />
  <ApplicableClasses>
    <ClassName>IfcFlowMeter/WATERMETER</ClassName>
  </ApplicableClasses>
  <ApplicableTypeValue>IfcFlowMeter/WATERMETER</ApplicableTypeValue>
  <PropertyDefs>
    <PropertyDef ifdguid="87df4600d1e811e1800000215ad4efdf">
      <Name>Type</Name>
      <Definition>Defines the allowed values for selection of the flow meter operation type.</Definition>
      <PropertyType>
        <TypePropertyEnumeratedValue>
          <EnumList name="PEnum_WaterMeterType">
            <EnumItem>COMPOUND</EnumItem>
            <EnumItem>INFERENTIAL</EnumItem>
            <EnumItem>PISTON</EnumItem>
            <EnumItem>OTHER</EnumItem>
            <EnumItem>NOTKNOWN</EnumItem>
            <EnumItem>UNSET</EnumItem>
          </EnumList>
          <ConstantList>
            <ConstantDef>
              <Name>COMPOUND</Name>
              <NameAliases>
                <NameAlias lang="en">Compound</NameAlias>
              </NameAliases>
              <DefinitionAliases />
            </ConstantDef>
            <ConstantDef>
              <Name>INFERENTIAL</Name>
              <NameAliases>
                <NameAlias lang="en">Inferential</NameAlias>
              </NameAliases>
              <DefinitionAliases />
            </ConstantDef>
            <ConstantDef>
              <Name>PISTON</Name>
              <NameAliases>
                <NameAlias lang="en">Piston</NameAlias>
              </NameAliases>
              <DefinitionAliases />
            </ConstantDef>
            <ConstantDef>
              <Name>OTHER</Name>
              <Definition>required category not on scale</Definition>
              <NameAliases>
                <NameAlias lang="en">(other)</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en">Value is not listed.</DefinitionAlias>
              </DefinitionAliases>
            </ConstantDef>
            <ConstantDef>
              <Name>NOTKNOWN</Name>
              <Definition>Value is unkown</Definition>
              <NameAliases>
                <NameAlias lang="en">(unknown)</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en">Value is unknown.</DefinitionAlias>
              </DefinitionAliases>
            </ConstantDef>
            <ConstantDef>
              <Name>UNSET</Name>
              <Definition>Value has not been specified</Definition>
              <NameAliases>
                <NameAlias lang="en">(unset)</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en">Value has not been specified.</DefinitionAlias>
              </DefinitionAliases>
            </ConstantDef>
          </ConstantList>
        </TypePropertyEnumeratedValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="en">Type</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="en" />
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="93327180d1e811e1800000215ad4efdf">
      <Name>ConnectionSize</Name>
      <Definition>Defines the size of inlet and outlet pipe connections to the meter.</Definition>
      <PropertyType>
        <TypePropertySingleValue>
          <DataType type="IfcPositiveLengthMeasure" />
        </TypePropertySingleValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="en">Connection Size</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="en" />
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="99c0e900d1e811e1800000215ad4efdf">
