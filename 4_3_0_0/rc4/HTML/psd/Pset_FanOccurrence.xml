  <PropertyDefs>
    <PropertyDef ifdguid="beff2880d1e311e1800000215ad4efdf">
      <Name>DischargeType</Name>
      <Definition>Defines the type of connection at the fan discharge.

Duct: Discharge into ductwork.
Screen: Discharge into screen outlet.
Louver: Discharge into a louver.
Damper: Discharge into a damper.</Definition>
      <PropertyType>
        <TypePropertyEnumeratedValue>
          <EnumList name="PEnum_FanDischargeType">
            <EnumItem>DUCT</EnumItem>
            <EnumItem>SCREEN</EnumItem>
            <EnumItem>LOUVER</EnumItem>
            <EnumItem>DAMPER</EnumItem>
            <EnumItem>OTHER</EnumItem>
            <EnumItem>NOTKNOWN</EnumItem>
            <EnumItem>UNSET</EnumItem>
          </EnumList>
          <ConstantList>
            <ConstantDef>
              <Name>DUCT</Name>
              <NameAliases>
                <NameAlias lang="en">Duct</NameAlias>
              </NameAliases>
              <DefinitionAliases />
            </ConstantDef>
            <ConstantDef>
              <Name>SCREEN</Name>
              <NameAliases>
                <NameAlias lang="en">Screen</NameAlias>
              </NameAliases>
              <DefinitionAliases />
            </ConstantDef>
            <ConstantDef>
              <Name>LOUVER</Name>
              <NameAliases>
                <NameAlias lang="en">Louver</NameAlias>
              </NameAliases>
              <DefinitionAliases />
            </ConstantDef>
            <ConstantDef>
              <Name>DAMPER</Name>
              <NameAliases>
                <NameAlias lang="en">Damper</NameAlias>
              </NameAliases>
              <DefinitionAliases />
            </ConstantDef>
            <ConstantDef>
              <Name>OTHER</Name>
              <Definition>required category not on scale</Definition>
              <NameAliases>
                <NameAlias lang="en">(other)</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en">Value is not listed.</DefinitionAlias>
              </DefinitionAliases>
            </ConstantDef>
            <ConstantDef>
              <Name>NOTKNOWN</Name>
              <Definition>Value is unkown</Definition>
              <NameAliases>
                <NameAlias lang="en">(unknown)</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en">Value is unknown.</DefinitionAlias>
              </DefinitionAliases>
            </ConstantDef>
            <ConstantDef>
              <Name>UNSET</Name>
              <Definition>Value has not been specified</Definition>
              <NameAliases>
                <NameAlias lang="en">(unset)</NameAlias>
              </NameAliases>
              <DefinitionAliases>
                <DefinitionAlias lang="en">Value has not been specified.</DefinitionAlias>
              </DefinitionAliases>
            </ConstantDef>
          </ConstantList>
        </TypePropertyEnumeratedValue>
      </PropertyType>
      <NameAliases>
        <NameAlias lang="en">Discharge Type</NameAlias>
        <NameAlias lang="ja-JP">吐出形式</NameAlias>
      </NameAliases>
      <DefinitionAliases>
        <DefinitionAlias lang="en" />
        <DefinitionAlias lang="ja-JP">送風機出口の接続形式定義
Duct：ダクトへの吐き出し
Screen:SCREEN内への吐き出し
Louver:ルーバーへの吐き出し</DefinitionAlias>
      </DefinitionAliases>
    </PropertyDef>
    <PropertyDef ifdguid="cfaf9e80d1e311e1800000215ad4efdf">
      <Name>ApplicationOfFan</Name>
