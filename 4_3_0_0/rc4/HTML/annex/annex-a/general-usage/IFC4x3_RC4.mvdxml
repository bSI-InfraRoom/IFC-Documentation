								<TemplateRule Description="Air supply leaving the evaporator." Parameters="PredefinedType[Value]=&apos;DIRECTEXPANSION&apos; AND PortName[Value]=&apos;AirOut&apos; AND SystemType[Value]=&apos;AIRCONDITIONING&apos; AND Flow[Value]=&apos;SOURCE&apos;" />
