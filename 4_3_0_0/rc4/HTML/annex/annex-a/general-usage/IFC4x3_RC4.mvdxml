								<TemplateRule Description="Rainwater." Parameters="PredefinedType[Value]=&apos;ROOFDRAIN&apos; AND PortName[Value]=&apos;Outlet&apos; AND SystemType[Value]=&apos;RAINWATER&apos; AND Flow[Value]=&apos;SOURCE&apos;" />
