								<TemplateRule Description="Physical elements contained in the building." />
								<TemplateRule Description="Annotations that are directly related to the building." />
								<TemplateRule Description="Grids that are directly related to the building." />
							</TemplateRules>
						</Concept>
						<Concept uuid="018e9e73-39f6-432a-8eb3-80fa71c23a62" name="Property Sets for Objects">
							<Template ref="f74255a6-0c0e-4f31-84ad-24981db62461" />
							<TemplateRules operator="and">
								<TemplateRule Parameters="PsetName[Value]=&apos;Pset_BuildingCommon&apos;" />
								<TemplateRule Parameters="PsetName[Value]=&apos;Pset_BuildingUse&apos;" />
								<TemplateRule Parameters="PsetName[Value]=&apos;Pset_BuildingUseAdjacent&apos;" />
