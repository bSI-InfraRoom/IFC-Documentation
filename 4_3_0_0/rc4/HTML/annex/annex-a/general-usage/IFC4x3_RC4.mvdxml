																	</EntityRules>
																</AttributeRule>
																<AttributeRule RuleID="Type" AttributeName="RepresentationType">
																	<EntityRules>
																		<EntityRule EntityName="IfcLabel" />
																	</EntityRules>
																</AttributeRule>
																<AttributeRule AttributeName="Items">
