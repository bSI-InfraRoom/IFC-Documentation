																		</AttributeRule>
																	</AttributeRules>
																</EntityRule>
															</EntityRules>
														</AttributeRule>
														<AttributeRule AttributeName="Name" />
														<AttributeRule AttributeName="Description" />
													</AttributeRules>
												</EntityRule>
											</EntityRules>
										</AttributeRule>
									</Rules>
