																							</EntityRules>
																						</AttributeRule>
																					</AttributeRules>
																				</EntityRule>
																			</EntityRules>
																		</AttributeRule>
																		<AttributeRule AttributeName="Benchmark">
																			<EntityRules>
																				<EntityRule EntityName="IfcBenchmarkEnum" />
																			</EntityRules>
																		</AttributeRule>
																		<AttributeRule AttributeName="ReferencePath">
																			<EntityRules>
																				<EntityRule EntityName="IfcReference">
																					<AttributeRules>
