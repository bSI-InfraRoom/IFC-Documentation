								<TemplateRule Description="Drainage inlet." Parameters="PredefinedType[Value]=&apos;FLOORTRAP&apos; AND PortName[Value]=&apos;Inlet&apos; AND SystemType[Value]=&apos;DRAINAGE&apos; AND Flow[Value]=&apos;SINK&apos;" />
